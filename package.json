{
  "name": "ts-translator-auto-core",
  "version": "1.1.3",
  "description": "DeepL translation management for multiple languages",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "test:modules": "node src/examples/dual-module-test.js",
    "test:esm": "node --experimental-modules src/examples/esm-example.js",
    "test:cjs": "node src/examples/cjs-example.cjs",
    "translate-languages": "node -r ts-node/register src/examples/translate-languages.js"
  },
  "keywords": [
    "translator",
    "typescript",
    "i18n",
    "localization"
  ],
  "author": "SeokyoungYou",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "chalk": "^4.1.2",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "tslib": "^2.6.2"
  }
}
